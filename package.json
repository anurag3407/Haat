{
  "name": "hatt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"yellow,blue\"",
    "dev:full": "node start-development.cjs",
    "dev:populate": "cd backend && node populate-database-complete.js && cd .. && npm run dev:both",
    "build": "vite build",
    "build:netlify": "npm run build",
    "build:backend": "cd backend && npm install --production",
    "build:full": "npm run build && npm run build:backend",
    "start": "npm run preview",
    "start:backend": "cd backend && npm start",
    "start:production": "concurrently \"npm run start:backend\" \"npm run start\" --names \"backend,frontend\" --prefix-colors \"yellow,blue\"",
    "lint": "eslint .",
    "preview": "vite preview --host 0.0.0.0",
    "deploy:setup": "npm run build:full && echo 'Build completed successfully!'",
    "deploy:netlify": "npm run build:netlify && echo 'Netlify build completed!'",
    "deploy:guide": "node deploy-netlify.cjs",
    "clean": "rm -rf dist node_modules backend/node_modules",
    "postinstall": "cd backend && npm install"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.23.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/poppins": "^5.2.6",
    "@google/maps": "^1.1.3",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.9",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.526.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.7.1",
    "react-toastify": "^11.0.5",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
